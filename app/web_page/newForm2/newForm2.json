    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.konfiguracija ){",
        "\treturn {",
        "\t\t//map konfiguracija variable to expected task contract input",
        "\t\tkonfiguracijaInput: {",
        "\t\t\ttipoviPropisa: $data.konfiguracija.tipoviPropisa !== undefined ? $data.konfiguracija.tipoviPropisa : null,",
